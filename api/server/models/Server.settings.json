{
  "connection": "default",
  "collectionName": "servers",
  "info": {
    "name": "server",
    "description": "Danh sách server sở hữu"
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "ip": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "username": {
      "type": "string"
    },
    "password": {
      "type": "string"
    },
    "servertype": {
      "model": "servertype",
      "via": "server"
    }
  }
}